{
    "name": "Pogify",
    "description": "Listen to music with your stream chat without getting DMCA-striked.",
    "version": "0.0.1",
    "manifest_version": 2,
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "browsingData",
        "activeTab",
        "notifications",
        "storage",
        "http://*/*",
        "https://*/*"
    ],
    "content_scripts": [
        {
            "js": [
                "agent.js"
            ],
            "matches": [
                "https://open.spotify.com/*"
            ]
        }
    ],
    "web_accessible_resources": [
        "modal.html",
        "firebase/*",
        "app.js"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "browser_action": {},
    "icons": {
        "32": "icon.png"
    }
}